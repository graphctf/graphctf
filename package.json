  "scripts": {
    "start": "DEBUG=graphctf:* node dist",
    "clean": "rm -rf dist",
    "build": "npm -s run clean && npm -s run generate && npx tsc",
    "generate": "prisma generate",
    "index": "create-index ./src -r -i -x ts -X ts -S -c '{\"mode\":\"*\"}'",
    "dev": "DEBUG=graphctf:* nodemon --watch 'src/**' --ext ts --exec 'ts-node src'",
    "debug": "DEBUG=* nodemon --watch 'src/**' --ext ts --exec 'ts-node src'"
  },
